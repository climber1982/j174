<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lovo.ss.dao.ISellDao">

     <insert id="sell" parameterType="OrderEntity">
        insert into sys_order(goodsName,goodsNum,sellPrice)
        values(#{goodsName},#{goodsNum},#{sellPrice})
     </insert>

</mapper>